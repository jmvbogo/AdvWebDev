extends layout

block content
  h1= title
  p Pick a Number to Generate #{title} 

    br
    
    form
        select#gridSize(name='gridSize')
          option(selected= true) Number
          option(value='3') 3
          option(value='4') 4
          option(value='5') 5
          option(value='10') 10
          option(value='20') 20
        br        
        input(type="submit")

    +makeTable(results)